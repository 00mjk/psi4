memory 4 gb
 
molecule dimer {
0 1
H 0.000000 0.803322 1.423647
F 0.000000 -0.045324 1.068702
--
0 1
H 0.000000 -0.121050 -0.219182
F 0.000000 0.009127 -1.132603

    units angstrom
    no_reorient
    symmetry c1
}


set globals {
    reference RHF
    e_convergence 1e-8
    d_convergence 1e-8
    basis jun-cc-pvdz
    scf_type mem_df
    df_basis_scf aug-cc-pvtz-jkfit
    df_basis_mp2 aug-cc-pvtz-ri
    SAPT_DFT_MP2_DISP_ALG fisapt
}

set SAPT_DFT_FUNCTIONAL HF
energy('sapt(dft)', molecule=dimer)

compare_values(0.00211996, variable('sapt exch-disp20(s^inf) energy'), 8, "SAPT EXCH-DISP20(S^inf) ENERGY")
