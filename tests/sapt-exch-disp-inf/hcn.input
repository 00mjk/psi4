memory 4 gb
 
molecule dimer {
0 1
H 0.000000 0.000000 3.409664
C 0.000000 0.000000 2.340950
N 0.000000 0.000000 1.185959
--
0 1
H 0.000000 0.000000 -0.148222
C 0.000000 0.000000 -1.222546
N 0.000000 0.000000 -2.379698

    units angstrom
    no_reorient
    symmetry c1
}


set globals {
    reference RHF
    e_convergence 1e-8
    d_convergence 1e-8
    basis jun-cc-pvdz
    scf_type mem_df
    df_basis_scf aug-cc-pvtz-jkfit
    df_basis_mp2 aug-cc-pvtz-ri
    SAPT_DFT_MP2_DISP_ALG fisapt
}

set SAPT_DFT_FUNCTIONAL HF
energy('sapt(dft)', molecule=dimer)

compare_values(0.00271925, variable('sapt exch-disp20(s^inf) energy'), 8, "SAPT EXCH-DISP20(S^inf) ENERGY")
