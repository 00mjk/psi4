# Any line starting with the # character is a comment line
#! Sample HF/cc-pVDZ H2O computation

memory 600 mb

molecule h2o {
  O 
  H 1 0.96
  H 1 0.96 2 104.5
}

set basis cc-pVDZ

set scf_type pk
set DF_SCF_GUESS false
energy('scf')
compare_values(-76.02665366, variable('SCF TOTAL ENERGY'), 4, 'SCF energy PK')  #TEST

set scf_type df
set DF_SCF_GUESS true
energy('scf')
compare_values(-76.0266327341067125, variable('SCF TOTAL ENERGY'), 6, 'SCF energy DF')  #TEST
