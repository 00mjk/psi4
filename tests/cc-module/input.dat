def do_check(module):
    set_options( {'basis':'cc-pvdz', 'cc_type':'conv', 'qc_module':module, 'freeze_core':'true', 'e_convergence': 10,
    'd_convergence': 8})
    ret_e = energy('ccsd(t)', molecule = hf)
    print_d = {}
    for var_name, ref_val in refs.items():
        compare_values(ref_val, get_variable(var_name), 7, "{} [{}]".format(module, var_name))
    compare_values(refs['CCSD(T) TOTAL ENERGY'], ret_e, 7, "{} [{}]".format(module, 'return value'))
    clean_variables()
    clean()


# compare ccsd/ccsd(t) variables across FNOCC/CCENERGY

# CCENERGY
do_check('CCENERGY')

# FNOCC
do_check('FNOCC')

